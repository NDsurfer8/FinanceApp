        {
          "photosPermission": "The app accesses your photos to create a profile picture for you.",
          "cameraPermission": "The app accesses your camera to take a profile picture for you."
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#6366f1"
        }
      ],
      "expo-apple-authentication",
      "expo-local-authentication",
