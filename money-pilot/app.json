{
  "expo": {
    "name": "VectorFi",
    "slug": "money-pilot",
    "version": "1.0.17",
    "orientation": "portrait",
    "icon": "./assets/ios/icon-1024.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
